<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logInVM" type="com.ehayvan.app.modules.login.data.viewmodel.LogInVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearLogIn"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearOnboardingCr"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameActionSheetCo"
                android:layout_height="@dimen/_378pxv"
                android:layout_marginTop="@dimen/_245pxv"
                android:layout_width="match_parent" style="@style/groupStyle">
                <LinearLayout android:id="@+id/linearActionSheet"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameStackvector"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_64pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="@dimen/_60pxh" style="@style/groupStylecornerRadius_4">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_27pxh"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginEnd="@dimen/_21pxh"
                            android:layout_marginStart="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconframe"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconframe"/>
                    </FrameLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearActionSheet1"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitle"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_127pxh"
                            android:layout_marginStart="@dimen/_128pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_17pxh"
                            android:singleLine="true"
                            android:text="@{logInVM.logInModel.txtTitle}"
                            style="@style/txtCatamaranbold26" tools:text="@string/lbl_log_in"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearInputBox"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearInputContent"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtLabel"
                                    android:layout_height="match_parent"
                                    android:layout_marginHorizontal="@dimen/_16pxh"
                                    android:layout_marginVertical="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{logInVM.logInModel.txtLabel}"
                                    style="@style/txtNotoSansnotosansregular16" tools:text="@string/lbl_your_email"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearInputBox1"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearInputContent1"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtLabelOne"
                                    android:layout_height="match_parent"
                                    android:layout_marginHorizontal="@dimen/_16pxh"
                                    android:layout_marginVertical="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{logInVM.logInModel.txtLabelOne}"
                                    style="@style/txtNotoSansnotosansregular16" tools:text="@string/lbl_password"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearPrimaryButton"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_24pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStyleblue_600cornerRadius">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtButton"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_17pxv"
                                android:layout_marginEnd="@dimen/_143pxh"
                                android:layout_marginStart="@dimen/_143pxh"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{logInVM.logInModel.txtButton}"
                                style="@style/txtNotoSansnotosansmedium14" tools:text="@string/lbl_log_in"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageActionSheetBa"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_378pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_actionsheetba_gray_50"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_actionsheetba_gray_50"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
