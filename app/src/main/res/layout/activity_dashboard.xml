<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dashboardVM" type="com.barsapplication.app.modules.dashboard.data.viewmodel.DashboardVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDashboard"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearDashboard1"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearStatusBarNa"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_50">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearNavigationHe"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnhello"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.94"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHello"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{dashboardVM.dashboardModel.txtHello}"
                                style="@style/txtNotoSansnotosansregular14" tools:text="@string/lbl_hello"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtEsther"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{dashboardVM.dashboardModel.txtEsther}"
                                style="@style/txtNotoSansnotosanssemibold16" tools:text="@string/lbl_esther"/>
                        </LinearLayout>
                        <ImageButton
                            android:id="@+id/btnSearch"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:scaleType="fitXY"
                            android:background="?android:attr/selectableItemBackground"
                            android:src="@drawable/img_search"
                            tools:ignore="ContentDescription"
                            android:contentDescription="deneme"
                            tools:src="@drawable/img_search" />
                        <View android:background="@color/gray_300"
                            android:id="@+id/lineDivider"
                            android:layout_height="@dimen/_19pxv"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_weight="0.0" android:layout_width="0dp"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <View
                    android:background="@drawable/rectangle_gradient_s_bluegray_400_87_e_bluegray_400_87"
                    android:id="@+id/lineLine"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_327pxh"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageEmptySpaceIll"
                android:layout_gravity="start"
                android:layout_height="@dimen/_210pxh"
                android:layout_marginEnd="@dimen/_59pxh"
                android:layout_marginStart="@dimen/_59pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="@dimen/_225pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_emptyspaceill"
                tools:ignore="ContentDescription" tools:src="@drawable/img_emptyspaceill"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTitle"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_17pxh"
                android:singleLine="true"
                android:text="@{dashboardVM.dashboardModel.txtTitle}"
                style="@style/txtCatamaranbold26" tools:text="@string/lbl_uh_oh"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtBody"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{dashboardVM.dashboardModel.txtBody}"
                style="@style/txtNotoSansnotosansmedium16" tools:text="@string/msg_looks_like_you"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearButtonsContain"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_89pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnAddPet"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_add_pet"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_add_pet"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
