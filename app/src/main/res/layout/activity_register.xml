<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerVM" type="com.ehayvan.app.modules.register.data.viewmodel.RegisterVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearRegister"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearOnboardingCr"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearActionSheetCo"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_69pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStacktitle"
                    android:layout_height="@dimen/_554pxv"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <LinearLayout android:id="@+id/linearActionSheet"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_75pxv"
                        android:layout_marginTop="@dimen/_75pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitle"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_17pxh"
                            android:singleLine="true"
                            android:text="@{registerVM.registerModel.txtTitle}"
                            style="@style/txtCatamaranbold26" tools:text="@string/lbl_sign_up"/>
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtBody"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:text="@{registerVM.registerModel.txtBody}"
                            style="@style/txtNotoSansnotosansmedium16" tools:text="@string/msg_welcome_please"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearInputBox"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearInputContent"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtNameandSurnam"
                                    android:layout_height="match_parent"
                                    android:layout_marginHorizontal="@dimen/_16pxh"
                                    android:layout_marginVertical="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{registerVM.registerModel.txtNameandSurnam}"
                                    style="@style/txtNotoSansnotosansregular16" tools:text="@string/msg_name_and_surnam"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearInputBox1"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearInputContent1"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtLabel"
                                    android:layout_height="match_parent"
                                    android:layout_marginHorizontal="@dimen/_16pxh"
                                    android:layout_marginVertical="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{registerVM.registerModel.txtLabel}"
                                    style="@style/txtNotoSansnotosansregular16" tools:text="@string/lbl_your_email"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearInputBox2"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearInputContent2"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtLabelOne"
                                    android:layout_height="match_parent"
                                    android:layout_marginHorizontal="@dimen/_16pxh"
                                    android:layout_marginVertical="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{registerVM.registerModel.txtLabelOne}"
                                    style="@style/txtNotoSansnotosansregular16" tools:text="@string/lbl_password"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowtext"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <androidx.appcompat.widget.AppCompatCheckBox
                                android:buttonTint="@color/bluegray_101"
                                android:ellipsize="end"
                                android:fontFamily="@font/noto_sans"
                                android:id="@+id/checkBoxText"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.47"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:paddingStart="@dimen/_8pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_pet_owner"
                                android:textColor="@color/bluegray_400"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_14pxh"
                                android:textStyle="bold" tools:text="@string/lbl_pet_owner"/>
                            <androidx.appcompat.widget.AppCompatCheckBox
                                android:buttonTint="@color/bluegray_101"
                                android:ellipsize="end"
                                android:fontFamily="@font/noto_sans"
                                android:id="@+id/checkBoxTextOne"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_31pxh"
                                android:layout_weight="0.53"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:paddingStart="@dimen/_8pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_veterinarian"
                                android:textColor="@color/bluegray_400"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_14pxh"
                                android:textStyle="bold" tools:text="@string/lbl_veterinarian"/>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatCheckBox
                            android:buttonTint="@color/bluegray_101"
                            android:ellipsize="end"
                            android:fontFamily="@font/noto_sans"
                            android:id="@+id/checkBoxTextTwo"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:paddingStart="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@string/msg_accept_terms_an"
                            android:textColor="@color/bluegray_400"
                            android:textFontWeight="400"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="bold" tools:text="@string/msg_accept_terms_an"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearPrimaryButton"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStyleblue_600">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtButton"
                                android:layout_height="match_parent"
                                android:layout_marginEnd="@dimen/_139pxh"
                                android:layout_marginStart="@dimen/_139pxh"
                                android:layout_marginTop="@dimen/_18pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{registerVM.registerModel.txtButton}"
                                style="@style/txtNotoSansnotosansmedium14" tools:text="@string/lbl_sign_up"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtConfirmation"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_73pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{registerVM.registerModel.txtConfirmation}"
                            style="@style/txtNotoSansnotosansmedium14_1" tools:text="@string/msg_already_have_an"/>
                    </LinearLayout>
                    <ImageButton
                        android:background="@drawable/rectangle_border_blue_50_radius_32"
                        android:id="@+id/btnIconFrame"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_142pxh"
                        android:layout_marginStart="@dimen/_142pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_64pxh"
                        android:paddingHorizontal="@dimen/_18pxh"
                        android:paddingVertical="@dimen/_18pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_iconframe" tools:ignore="ContentDescription"/>
                    <ImageView android:id="@+id/imageActionSheetBa"
                        android:layout_gravity="start"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_actionsheetba}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_actionsheetba"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
