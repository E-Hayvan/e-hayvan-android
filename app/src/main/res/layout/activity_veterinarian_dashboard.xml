<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="veterinarianDashboardVM" type="com.ehayvan.app.modules.veterinariandashboard.data.viewmodel.VeterinarianDashboardVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearVeterinarianDa"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearVeterinarianDa1"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearStatusBarNa"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_50">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <FrameLayout android:id="@+id/frameNavigationHead"
                        android:layout_height="@dimen/_44pxv" android:layout_width="match_parent">
                        <View
                            android:background="@drawable/rectangle_gradient_s_bluegray_400_87_e_bluegray_400_87"
                            android:id="@+id/lineLine"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_1pxh" android:layout_width="@dimen/_327pxh"/>
                        <LinearLayout
                            android:id="@+id/linearNavigationHe"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnhello"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.94"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtHello"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_width="wrap_content"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{veterinarianDashboardVM.veterinarianDashboardModel.txtHello}"
                                    style="@style/txtNotoSansnotosansregular14" tools:text="@string/lbl_hello"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtAlice"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="wrap_content"
                                    android:lineSpacingExtra="@dimen/_6pxh"
                                    android:singleLine="true"
                                    android:text="@{veterinarianDashboardVM.veterinarianDashboardModel.txtAlice}"
                                    style="@style/txtNotoSansnotosanssemibold16" tools:text="@string/lbl_alice"/>
                            </LinearLayout>
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                            <View android:background="@color/gray_300"
                                android:id="@+id/lineDivider"
                                android:layout_height="@dimen/_19pxv"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginStart="@dimen/_15pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_weight="0.0" android:layout_width="0dp"/>
                        </LinearLayout>
                    </FrameLayout>
                </androidx.appcompat.widget.Toolbar>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearNavigationSe"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.87"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{veterinarianDashboardVM.veterinarianDashboardModel.txtText}"
                    style="@style/txtNotoSansnotosanssemibold16" tools:text="@string/msg_today_s_appoint"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnThree"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_8pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_4pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_3"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_3"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListtitle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_24pxh}" tools:itemCount="3" tools:listitem="@layout/row_listtitle"/>
        </LinearLayout>
    </LinearLayout>
</layout>
